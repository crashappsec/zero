{
  "technology": "Angular",
  "category": "web-frameworks/frontend",
  "confidence": 85,
  "patterns": [
    {
      "language": "typescript",
      "file_extensions": [".ts"],
      "patterns": [
        {
          "regex": "import\\s+\\{[^}]*(Component|NgModule|Injectable|Directive)[^}]*\\}\\s+from\\s+['\"]@angular/core['\"]",
          "description": "Angular core decorators",
          "example": "import { Component, NgModule } from '@angular/core';"
        },
        {
          "regex": "import\\s+.*\\s+from\\s+['\"]@angular/(core|common|router|forms)['\"]",
          "description": "Angular package imports",
          "example": "import { RouterModule } from '@angular/router';"
        },
        {
          "regex": "@Component\\(",
          "description": "Component decorator usage",
          "example": "@Component({ selector: 'app-root' })"
        },
        {
          "regex": "@NgModule\\(",
          "description": "NgModule decorator usage",
          "example": "@NgModule({ declarations: [AppComponent] })"
        },
        {
          "regex": "@Injectable\\(",
          "description": "Injectable service decorator",
          "example": "@Injectable({ providedIn: 'root' })"
        }
      ]
    }
  ],
  "common_imports": [
    "Component",
    "NgModule",
    "Injectable",
    "Directive",
    "Pipe",
    "Input",
    "Output",
    "EventEmitter",
    "OnInit",
    "RouterModule",
    "FormsModule",
    "ReactiveFormsModule",
    "HttpClient"
  ],
  "detection_notes": [
    "Angular is TypeScript-first framework",
    "Decorators (@Component, @NgModule) are distinctive Angular patterns",
    "All imports from @angular/* namespace",
    "Angular rarely uses plain JavaScript files",
    "Look for .component.ts, .service.ts, .module.ts naming"
  ]
}
