{
  "technology": "Express",
  "category": "web-frameworks/backend",
  "confidence": 85,
  "patterns": [
    {
      "language": "javascript",
      "file_extensions": [".js", ".mjs"],
      "patterns": [
        {
          "regex": "const\\s+express\\s*=\\s*require\\(['\"]express['\"]\\)",
          "description": "CommonJS require",
          "example": "const express = require('express');"
        },
        {
          "regex": "import\\s+express\\s+from\\s+['\"]express['\"]",
          "description": "ES6 default import",
          "example": "import express from 'express';"
        },
        {
          "regex": "const\\s+app\\s*=\\s*express\\(\\)",
          "description": "Express app initialization",
          "example": "const app = express();"
        },
        {
          "regex": "app\\.(get|post|put|delete|patch)\\(['\"]",
          "description": "Express route definition",
          "example": "app.get('/api/users', handler);"
        },
        {
          "regex": "app\\.use\\(",
          "description": "Express middleware usage",
          "example": "app.use(express.json());"
        },
        {
          "regex": "app\\.listen\\(",
          "description": "Express server start",
          "example": "app.listen(3000, () => {});"
        }
      ]
    },
    {
      "language": "typescript",
      "file_extensions": [".ts"],
      "patterns": [
        {
          "regex": "import\\s+express\\s+from\\s+['\"]express['\"]",
          "description": "ES6 import",
          "example": "import express from 'express';"
        },
        {
          "regex": "import\\s+\\{\\s*Request,\\s*Response\\s*\\}\\s+from\\s+['\"]express['\"]",
          "description": "Express type imports",
          "example": "import { Request, Response } from 'express';"
        },
        {
          "regex": ":\\s*express\\.Application",
          "description": "Express Application type",
          "example": "const app: express.Application = express();"
        }
      ]
    }
  ],
  "common_methods": [
    "app.get",
    "app.post",
    "app.put",
    "app.delete",
    "app.patch",
    "app.use",
    "app.listen",
    "app.route",
    "express.Router",
    "express.json",
    "express.urlencoded",
    "express.static"
  ],
  "middleware_patterns": [
    "app.use(express.json())",
    "app.use(express.urlencoded({ extended: true }))",
    "app.use(cors())",
    "app.use(morgan('combined'))"
  ]
}
