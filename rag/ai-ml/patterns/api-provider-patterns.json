{
  "version": "1.0.0",
  "description": "LLM API provider detection patterns",
  "providers": {
    "openai": {
      "name": "OpenAI",
      "env_vars": ["OPENAI_API_KEY"],
      "packages": ["openai"],
      "model_patterns": [
        {
          "pattern": "model\\s*[=:]\\s*[\"'](gpt-4[^\"']*|gpt-3\\.5[^\"']*|text-embedding[^\"']*|dall-e[^\"']*|whisper[^\"']*|o1[^\"']*)[\"']",
          "prefix": "openai/"
        }
      ],
      "api_key_pattern": "sk-[a-zA-Z0-9]{20,}"
    },
    "anthropic": {
      "name": "Anthropic",
      "env_vars": ["ANTHROPIC_API_KEY"],
      "packages": ["anthropic"],
      "model_patterns": [
        {
          "pattern": "model\\s*[=:]\\s*[\"'](claude-[^\"']+)[\"']",
          "prefix": "anthropic/"
        }
      ],
      "api_key_pattern": "sk-ant-[a-zA-Z0-9-]{20,}"
    },
    "google": {
      "name": "Google AI",
      "env_vars": ["GOOGLE_API_KEY", "GEMINI_API_KEY"],
      "packages": ["google-generativeai", "vertexai"],
      "model_patterns": [
        {
          "pattern": "model\\s*[=:]\\s*[\"'](gemini-[^\"']+)[\"']",
          "prefix": "google/"
        }
      ],
      "api_key_pattern": "AIza[a-zA-Z0-9_-]{35}"
    },
    "cohere": {
      "name": "Cohere",
      "env_vars": ["COHERE_API_KEY"],
      "packages": ["cohere"],
      "model_patterns": [
        {
          "pattern": "model\\s*[=:]\\s*[\"'](command[^\"']*|embed[^\"']*)[\"']",
          "prefix": "cohere/"
        }
      ],
      "api_key_pattern": "[a-zA-Z0-9]{40}"
    },
    "mistral": {
      "name": "Mistral",
      "env_vars": ["MISTRAL_API_KEY"],
      "packages": ["mistralai"],
      "model_patterns": [
        {
          "pattern": "model\\s*[=:]\\s*[\"'](mistral-[^\"']+|open-mistral[^\"']+|open-mixtral[^\"']+)[\"']",
          "prefix": "mistral/"
        }
      ],
      "api_key_pattern": "[a-zA-Z0-9]{32}"
    },
    "replicate": {
      "name": "Replicate",
      "env_vars": ["REPLICATE_API_TOKEN"],
      "packages": ["replicate"],
      "model_patterns": [],
      "api_key_pattern": "r8_[a-zA-Z0-9]{40}"
    },
    "together": {
      "name": "Together AI",
      "env_vars": ["TOGETHER_API_KEY"],
      "packages": ["together"],
      "model_patterns": [
        {
          "pattern": "together\\.Complete[^(]*\\([^)]*model\\s*=\\s*[\"']([^\"']+)[\"']",
          "prefix": "together/"
        }
      ],
      "api_key_pattern": "[a-f0-9]{64}"
    },
    "groq": {
      "name": "Groq",
      "env_vars": ["GROQ_API_KEY"],
      "packages": ["groq"],
      "model_patterns": [
        {
          "pattern": "Groq\\s*\\([^)]*\\)\\.chat[^(]*\\([^)]*model\\s*=\\s*[\"']([^\"']+)[\"']",
          "prefix": "groq/"
        }
      ],
      "api_key_pattern": "gsk_[a-zA-Z0-9]{52}"
    },
    "perplexity": {
      "name": "Perplexity",
      "env_vars": ["PERPLEXITY_API_KEY"],
      "packages": ["openai"],
      "model_patterns": [
        {
          "pattern": "model\\s*[=:]\\s*[\"'](llama-[^\"']*-sonar[^\"']*|pplx-[^\"']+)[\"']",
          "prefix": "perplexity/"
        }
      ],
      "api_key_pattern": "pplx-[a-zA-Z0-9]{48}"
    },
    "fireworks": {
      "name": "Fireworks AI",
      "env_vars": ["FIREWORKS_API_KEY"],
      "packages": ["fireworks-ai"],
      "model_patterns": [
        {
          "pattern": "model\\s*[=:]\\s*[\"'](accounts/fireworks/models/[^\"']+)[\"']",
          "prefix": ""
        }
      ],
      "api_key_pattern": "fw_[a-zA-Z0-9]{43}"
    },
    "anyscale": {
      "name": "Anyscale",
      "env_vars": ["ANYSCALE_API_KEY"],
      "packages": ["openai"],
      "model_patterns": [
        {
          "pattern": "base_url\\s*=\\s*[\"']https://api\\.endpoints\\.anyscale\\.com",
          "prefix": "anyscale/"
        }
      ],
      "api_key_pattern": "esecret_[a-zA-Z0-9]{32}"
    },
    "deepinfra": {
      "name": "DeepInfra",
      "env_vars": ["DEEPINFRA_API_TOKEN"],
      "packages": ["openai"],
      "model_patterns": [
        {
          "pattern": "base_url\\s*=\\s*[\"']https://api\\.deepinfra\\.com",
          "prefix": "deepinfra/"
        }
      ],
      "api_key_pattern": "[a-zA-Z0-9]{43}"
    }
  },
  "langchain_patterns": [
    {
      "pattern": "Chat(?:OpenAI|Anthropic|Google|VertexAI|Cohere|Mistral|Groq|Fireworks|Together|Perplexity)\\s*\\([^)]*model(?:_name)?\\s*=\\s*[\"']([^\"']+)[\"']",
      "extract_group": 1,
      "source": "api"
    }
  ]
}
